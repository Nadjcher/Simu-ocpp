  "scripts": {
    "dev:ui": "node ./node_modules/vite/bin/vite.js --port 3002 --host",
    "dev:runner": "npm --prefix ../perf-runner run dev",
    "dev:all": "concurrently -k -n runner,ui -c magenta,cyan \"npm --prefix ../perf-runner run dev\" \"npm run dev:ui\"",
    "start:all": "concurrently -k -n runner,ui -c magenta,cyan \"npm --prefix ../perf-runner run start\" \"node ./node_modules/vite/bin/vite.js --port 3002 --host\"",
    "build": "tsc -noEmit && vite build",
    "preview": "node ./node_modules/vite/bin/vite.js preview --port 4173 --host",
    "typecheck": "tsc -noEmit"
  },
